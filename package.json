{
  "name": "digbot-nest",
  "version": "1.6.12",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "dev": "nest start --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:create": "pnpx ts-node node_modules/@mikro-orm/cli/cli.js migration:create",
    "migration:up": "pnpx ts-node node_modules/@mikro-orm/cli/cli.js migration:up",
    "migration:down": "pnpx ts-node node_modules/@mikro-orm/cli/cli.js migration:down",
    "migration:list": "pnpx ts-node node_modules/@mikro-orm/cli/cli.js migration:list",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "ts-node node_modules/@mikro-orm/cli/cli.js migration:up && node dist/src/main",
    "test": "jest --coverage",
    "test:wsl": "jest --coverage --maxWorkers=4",
    "test:ci": "jest --ci --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "engines": {
    "npm": ">=10.5.0",
    "pnpm": ">=9.1.2",
    "node": ">=18.20.1 <21.0.0"
  },
  "dependencies": {
    "@discord-nestjs/common": "^5.2.12",
    "@discord-nestjs/core": "^5.3.14",
    "@mikro-orm/core": "5.9.8",
    "@mikro-orm/mariadb": "5.9.8",
    "@mikro-orm/migrations": "5.9.8",
    "@mikro-orm/nestjs": "^5.2.3",
    "@mikro-orm/seeder": "5.9.8",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/common": "9.4.3",
    "@nestjs/config": "2.3.4",
    "@nestjs/core": "9.4.3",
    "@nestjs/schedule": "^3.0.4",
    "@sentry/node": "^8.4.0",
    "@sentry/profiling-node": "^8.4.0",
    "axios": "^1.6.8",
    "bufferutil": "^4.0.8",
    "class-transformer": "0.5.1",
    "discord.js": "^14.14.1",
    "lodash": "^4.17.21",
    "mysql2": "^3.9.4",
    "ps2census": "^4.6.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "5.0.7",
    "rxjs": "7.8.1",
    "utf-8-validate": "^6.0.3",
    "websocket-ts": "^1.1.1"
  },
  "devDependencies": {
    "@mikro-orm/cli": "5.9.8",
    "@nestjs/cli": "9.5.0",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "^9.4.2",
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.12",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5"
  }
}
